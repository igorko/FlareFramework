<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="FlareFramework" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Release">
				<Option output="bin/Release/FlareGDE" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="$(#wx)/lib/gcc_dll/msw" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#wx)/lib/gcc_dll/msw" />
				</ResourceCompiler>
				<Linker>
					<Add option="-s" />
					<Add library="libwxmsw28_core.a" />
					<Add library="libwxbase28.a" />
					<Add library="libwxpng.a" />
					<Add library="libwxzlib.a" />
					<Add directory="$(#wx)/lib/gcc_dll" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-D__GNUWIN32__" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option='[[if (PLATFORM == PLATFORM_MSW &amp;&amp; (GetCompilerFactory().GetCompilerVersionString(_T(&quot;gcc&quot;)) &gt;= _T(&quot;4.0.0&quot;))) print(_T(&quot;-Wno-attributes&quot;));]]' />
			<Add option="-Wall" />
			<Add directory="$(#wx)/include" />
			<Add directory="$(#wx)/contrib/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#wx)/include" />
		</ResourceCompiler>
		<Linker>
			<Add option="-mthreads" />
		</Linker>
		<Unit filename="API/FileParser.cpp" />
		<Unit filename="API/FileParser.h" />
		<Unit filename="API/ItemManager.cpp" />
		<Unit filename="API/ItemManager.h" />
		<Unit filename="API/UtilsFileSystem.cpp" />
		<Unit filename="API/UtilsFileSystem.h" />
		<Unit filename="API/UtilsParsing.cpp" />
		<Unit filename="API/UtilsParsing.h" />
		<Unit filename="FlareFrameworkApp.cpp" />
		<Unit filename="FlareFrameworkApp.h" />
		<Unit filename="FlareFrameworkMain.cpp" />
		<Unit filename="FlareFrameworkMain.h" />
		<Unit filename="resource.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="wxsmith/FlareFrameworkframe.wxs" />
		<Extensions>
			<code_completion />
			<debugger />
			<wxsmith version="1">
				<gui name="wxWidgets" src="FlareGDEApp.cpp" main="FlareGDEFrame" init_handlers="necessary" language="CPP" />
				<resources>
					<wxFrame wxs="wxsmith/FlareGDEframe.wxs" src="FlareGDEMain.cpp" hdr="FlareGDEMain.h" fwddecl="0" i18n="1" name="FlareGDEFrame" language="CPP" />
				</resources>
			</wxsmith>
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
